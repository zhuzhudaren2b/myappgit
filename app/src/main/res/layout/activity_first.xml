<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:clipChildren="false"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <!--<com.example.zhangzhu.myapplication.CustomView.TitleLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        />-->
    <Button
        android:id = "@+id/jumpButton"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:background="#fdb60a"
        android:textSize="24sp"
        android:text="Jump" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:gravity="center"
        android:layout_marginTop="30dp"
        android:textSize="24sp"
        android:background="#8022a149"
        android:text="JumpJump" />

    <TextView
        android:id="@+id/textView_2"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:gravity="center"
        android:layout_marginTop="30dp"
        android:textSize="24sp"
        android:background="#0091ea"
        android:text="JumpJumpJump" />

    <com.example.zhangzhu.myapplication.MyRelativeLayout
        android:id="@+id/layout_sub"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:gravity="center"
        android:layout_marginTop="30dp"
        android:textSize="24sp"
        android:background="@color/colorPrimary">

        <com.example.zhangzhu.myapplication.MyTextView
            android:id = "@+id/layout_sub_jump"
            android:layout_width="80dp"
            android:layout_height="40dp"
            android:background="#fdb60a"
            android:textSize="24sp"
            android:text="Jump" />

    </com.example.zhangzhu.myapplication.MyRelativeLayout>

    <com.example.zhangzhu.myapplication.TestPoPLayout
        android:id="@+id/test_pop_layout"
        android:layout_width="264dp"
        android:layout_height="34dp"
        android:visibility="gone"
        />

    <com.example.zhangzhu.myapplication.CustomView.SuccessCatchPushView
        android:id="@+id/success_catch_push"
        android:layout_width="300dp"
        android:layout_height="46dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="240dp"
        android:visibility="gone"/>


</LinearLayout>
